		<shape>
			<gradient android:angle="90" android:startColor="#5a6a87"
				android:endColor="#364257" android:type="linear" />
		</shape>
	</item>

	<item android:bottom="2dp" android:top="2dp" android:right="2dp"
		android:left="2dp">
		<shape>
			<gradient android:angle="90" android:startColor="#748ab0"
				android:endColor="#495874" android:type="linear" />
		</shape>
	</item>

</layer-list>