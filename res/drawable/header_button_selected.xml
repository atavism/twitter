<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item>
		<shape>
			<gradient android:angle="90" android:startColor="#1a1a1a"
				android:endColor="#572626" android:type="linear" />
		</shape>
	</item>

	<item android:bottom="1dp" android:top="1dp" android:right="1dp"
		android:left="1dp">
		<shape>
			<gradient android:angle="90" android:startColor="#6a4646"
				android:endColor="#773635" android:type="linear" />
		</shape>
	</item>

	<item android:bottom="2dp" android:top="2dp" android:right="2dp"
		android:left="2dp">
		<shape>
			<gradient android:angle="90" android:startColor="#aa6f70"
				android:endColor="#8c3f3f" android:type="linear" />
		</shape>
	</item>

</layer-list>